{
  "name": "chatgpt-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=development tsc && node  dist/server.js",
    "dev": "NODE_ENV=development nodemon",
    "build": "tsc && rm -rf ./dist-pkg && pkg . && cp -r conf.d dist-pkg/ && tar -zcvf dist-pkg.tar.gz ./dist-pkg"
  },
  "author": "blackteam_xingyu",
  "license": "MIT",
  "bin": "dist/server.js",
  "pkg": {
    "options": [
      "experimental-modules",
      "es-module-specifier-resolution=node"
    ],
    "scripts": "dist/**/*.js",
    "targets": [
      "node16-linux-x64"
    ],
    "outputPath": "dist-pkg"
  },
  "dependencies": {
    "axios": "0.27.2",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-json-error": "^3.1.2",
    "koa-onerror": "^4.2.0",
    "koa-parameter": "^3.0.1",
    "koa-router": "^12.0.0",
    "koa2-cors": "^2.0.6",
    "log4js": "^6.9.1",
    "moment": "^2.29.4",
    "tunnel": "^0.0.6",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.6",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^20.2.3",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
